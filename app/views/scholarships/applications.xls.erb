<% for scholarship_application in @scholarship.scholarship_applications %>
<ss:Row>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.participant_id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.participant.try(:fullname) %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.participant.try(:grad_year) %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.participant.try(:high_school_name) %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship_id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.title %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.organization_name %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="Currency"><ss:Data ss:Type="Number"><%=h scholarship_application.amount %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.nominated %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.awarded %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.accepted %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.application_due_date %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.date_applied %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.restrictions %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.notes %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.renewable %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.renewable_years %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.full_ride %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.gap_funding %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.living_stipend %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.institution_id %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="Currency"><ss:Data ss:Type="Number"><%=h scholarship_application.scholarship.default_amount %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.default_renewable_years %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.default_full_ride %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.default_gap_funding %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.default_living_stipend %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h scholarship_application.scholarship.default_renewable %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="ShortDate"><ss:Data ss:Type="DateTime"><%=h scholarship_application.created_at.to_s(:excel) %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="ShortDate"><ss:Data ss:Type="DateTime"><%=h scholarship_application.updated_at.to_s(:excel) %></ss:Data></ss:Cell>
</ss:Row>
<% end %>
