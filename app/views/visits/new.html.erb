<h1>New High School Visit for <%= @high_school.name %></h1>

<% semantic_form_for :visit, @visit, :url => high_school_visits_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "events/fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator %>
	<%= link_to 'Cancel', high_school_visits_path(@high_school) %>

<% end %>
