<h1>Test Types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Maximum Score</th>
	<th colspan=3></th>
  </tr>

<% for test_type in @test_types %>
  <tr>
    <td><%=h test_type.name %></td>
    <td><%=h test_type.maximum_total_score %></td>
    <td><%= link_to 'Show', test_type %></td>
    <td><%= link_to 'Edit', edit_test_type_path(test_type) %></td>
    <td><%= link_to 'Destroy', test_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New test type', new_test_type_path, :class => "add button" %>
