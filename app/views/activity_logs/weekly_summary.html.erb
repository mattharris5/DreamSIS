weekly summary

<%= @activity_logs.size %>


<div id="sidebar">
	
	<p><%= link_to "Previous Week", activity_log_weekly_summary_path(@start_date.year, (@start_date - 7.days).month, (@start_date - 7.days).day), :class => "back button" %></p>
	
	<p><%= link_to "This Week", activity_log_current_week_summary_path, :class => "ok button" unless @start_date.this_week? %></p>
	
	<p><%= link_to "Next Week", activity_log_weekly_summary_path(@start_date.year, (@start_date + 7.days).month, (@start_date + 7.days).day), :class => "forward button" %></p>
	
</div>