<h1>Programs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Abbreviation</th>
	<th colspan=3></th>
  </tr>

<% for program in @programs %>
  <tr>
    <td><%=h program.title %></td>
    <td><%=h program.abbreviation %></td>
    <td><%= link_to 'Show', program %></td>
    <td><%= link_to 'Edit', edit_program_path(program) %></td>
    <td><%= link_to 'Destroy', program, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New program', new_program_path, :class => "add button" %>
