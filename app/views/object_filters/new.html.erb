<h1>New Filter</h1>

<% form_for :object_filter, @object_filter, :url => object_filters_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %> - or -
	<%= link_to 'Cancel', object_filters_path %>

<% end %>
