<h1>New Mentor Group</h1>

<% semantic_form_for @mentor_quarter_group do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<span class="separator">&mdash; or &mdash;</span>
	<%= link_to 'Cancel', mentor_quarter_groups_path %>

<% end %>
