<% @mentors.each do |mentor| %>
<ss:Row>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.firstname %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.middlename %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.lastname %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.nickname %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.display_name %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.email %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.birthdate %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.sex %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.type %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.previous_participant_id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.ferpa_agreement_signed_at %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.passed_background_check? %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.risk_form_signed_at %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.reg_id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.uw_student_no %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.uw_net_id %></ss:Data></ss:Cell>
	<ss:Cell><ss:Data ss:Type="String"><%=h mentor.mentor_term_groups.collect(&:term_id).flatten.uniq.size %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="ShortDate"><ss:Data ss:Type="DateTime"><%=h mentor.created_at.to_s(:excel) %></ss:Data></ss:Cell>
	<ss:Cell ss:StyleID="ShortDate"><ss:Data ss:Type="DateTime"><%=h mentor.updated_at.to_s(:excel) %></ss:Data></ss:Cell>
</ss:Row>
<% end %>
